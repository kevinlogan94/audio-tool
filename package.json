{
  "name": "audio-tool",
  "version": "1.0.3",
  "main": "dist/audio-tool.min.js",
  "description": "A native audio custom web component",
  "author": "Kevin Logan <kevinmlogan7@gmail.com> (kevinmlogan.com)",
  "scripts": {
    "start": "npm run build && npm run minify && npm test",
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "minify": "./node_modules/.bin/minify dist/audio-tool.js --out-file dist/audio-tool.min.js",
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "tests": "karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinlogan94/audio-tool"
  },
